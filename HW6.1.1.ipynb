    "import pyttsx3\n",
    "engine=pyttsx3.init()\n",
    "engine.setProperty('rate',160)\n",
    "engine.setProperty('volume',0.8)\n",
    "engine.say(\"You've got mail!\")\n",
    "engine.say(\"生日快樂\")\n",
    "engine.runAndWait()"
